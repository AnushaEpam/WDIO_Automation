{
    "env": {
      "es2021": true
    },
    "extends": ["google"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "require-jsdoc": "off",
      "valid-jsdoc": ["error", {
        "requireParamType": false
    }],
    "max-len": ["error", {
      "code": 120
    }],
    "new-cap": ["error", {"newIsCap": true, "capIsNew": false}]
    }
  }
  